# .env.example - Template for ZenType

# SQLite database URL (use this if DatabaseManager is active)
DATABASE_URL=sqlite:///~/.zentype/data/zentype.db

# JSON data storage location (use this if DataManager is active)
RESULTS_FILE_PATH=~/.zentype/data/typing_results.json

# Debug mode (True for development, False for production)
DEBUG=True

# Application environment (development/production)
ENVIRONMENT=development
